<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        table{
            border-collapse:collapse;
            width: 100%;
        }
        td,th{
            border: 1px solid red;
            text-align: left;
            padding: 0.5rem;
        }
        tr:nth-child(even){
            background-color: #dddddd;
        }
        
    </style>
</head>
<body>
    
    <table>
        <tr>
            <th>Name</th>
            <th>Origin</th>
            <th>Destination</th>
        </tr>
            <% People.forEach(function(people){%>
            <tr>
                <td>
                    <%= people.name %>
                </td>
                <td>
                    <%= people.origin %>
                </td>
                <td>
                    <%= people.destination %>
                </td>
            </tr>
        <% });  %>
            
        </table>
    
    
    
</body>
</html>